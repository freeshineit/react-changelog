{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts",
        "coverage/lcov-report/*.js"
      ]
    },
    "rules": {
        "ordered-imports": [false], // 排序
        "object-literal-sort-keys": [false], //// 排序
        "interface-name" : [true, "never-prefix"], // interface 命名前缀
        "class-name": false,
        "member-access": false,
        "no-console": [true, "error"]
    }
  }
  